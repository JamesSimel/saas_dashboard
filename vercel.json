{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next",
      "config": {
        "skipBuildCache": true,
        "nodeVersion": "22.0.0"
      }
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  }
}